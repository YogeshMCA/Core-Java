package com.example.Generics;
/*
 * We have to ensure that the Collections class in the jar file only accept String.
 */
public class WhyGenerics {
	
	public static void main(String arg[]){
		Collections<String> collections = new Collections<String>(5);
		//collections.add("A");
		//collections.add("B");
		//collections.add(12); //Without Generic - java.lang.ClassCastException
		//display(collections);
		
		
		Collections<Integer> intCollections = new Collections<Integer>(5);
		//intCollections.add("A");
		//intCollections.add("B");
		intCollections.add(12); //Without Generic - java.lang.ClassCastException
		display(intCollections);
		
		
				
	}
	public static  void display(Collections<Integer> collections){
		System.out.println(collections.getValues());
	}

}
