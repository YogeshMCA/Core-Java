import java.util.ArrayList;
import java.util.Collection;
import java.util.List;


/*
 * List extends Collection Interface
Collection - You can add stuff, remove stuff, iterate over stuff and query how much stuff is in there.

List - You can get the element at position n, you can add an element at position n, you can remove the element at position n.
 */
public class ListCollection {
public static void main(String ar[]){
	String st1 = "Name1";
	String st2 = "Name2";
	
	Collection<String> str = new ArrayList<>();
	str.add(st1);
	str.add(st2);
	
	List<String> ss = new ArrayList<>();
	ss.add(0, st1);
}
}
