import java.util.HashSet;
import java.util.Set;

public class SetCollection {

	public static void main(String arg[]) {
		
		//Mutable - Able to modify the object..
		StringBuilder name= new StringBuilder("AAAA");
		System.out.println("Name:"+name+" "+System.identityHashCode(name));
		name.append("XXX");
		System.out.println("Name:"+name+" "+System.identityHashCode(name));

		//Immutable - Not able to modify the object..
		String immu = new String("Imm");
		System.out.println("Immutable:"+immu+" "+System.identityHashCode(immu));
		
		Set<StringBuilder> uniqSet = new HashSet<>();
		uniqSet.add(name);
		uniqSet.add(new StringBuilder("AAAA"));
		System.out.println(uniqSet);
	}
}
